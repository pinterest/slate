{    
    "parser": "@typescript-eslint/parser",
    "extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended"],
    "env": {
        "browser": true,
        "commonjs": true,
        "node": true,
        "es6": true,
        "jest": true
    },
    "rules": {
        "react/prop-types": 0,
        "react-hooks/exhaustive-deps": 0,
        "eqeqeq": 0,
        "@typescript-eslint/no-var-requires": 0,
        "no-unused-vars": 1,
        "@typescript-eslint/no-unused-vars": 1
    }
}